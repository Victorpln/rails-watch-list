<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>My lists</h1>
      <ul class="list-group">
        <% @lists.each do |list| %>
          <li class="list-group-item">
            <%= link_to list.name, list_path(list) %>
          </li>
         <%= link_to "Add a new List", new_list_path %>
        <% end %>
      </ul>
    </div>
  </div>
</div>
